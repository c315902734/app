.sidebar-list-2 li {
  float: left;
  height: 26px;
  line-height: 26px;
  margin: 0 7px 8px 0;
  width: 45%;

  list-style: outside none none;
  vertical-align: top;
}
.sidebar-list-2 li a {
  background-color: #3f3f3f;
  border-radius: 2px;
  color: #d0d0d0;
  display: block;
  overflow: hidden;
  padding: 0 2px;
  text-align: center;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-decoration: none;
}

.sidebar-list-2 li a:hover {
    background-color: #f60;
    color: #fff;
        
    
}


.nav-list > li {
padding:10px;
background: #2b2b2b;
}

.video-list-item {
  border-bottom: 3px solid transparent;
  float: left;
  overflow: hidden;
  position: relative;
  transition: border 400ms ease-in-out 0s;
  margin-bottom: 22px;
}
.video-list-item .video-list-item-wrap {
  background-color: #fff;
  box-sizing: content-box;
  color: #333;
  display: block;
  padding-bottom: 55px;
  position: relative;
  width: 100%;
}
.video-list-item .video-cover {
  overflow: hidden;
  position: relative;
  width: 100%;
}
.video-list-item .video-cover .video-img {
  display: block;
  height: auto;
  width: 100%;
}
.video-list-item .video-cover .video-overlay {
  background-color: #000;
  bottom: 0;
  left: 0;
  opacity: 0;
  position: absolute;
  right: 0;
  top: 0;
  transition: all 250ms ease-out 0s;
  z-index: 10;
}
.bg_h{
    background: #f2f2f2;
}
.video-list-item .video-cover .video-play {
  background-image: url("http://i9.pdim.gs/8d743bdec3c8a4b6730d1bc6b0cfab18.png");
  height: 50px;
  left: 50%;
  margin-left: -25px;
  margin-top: -25px;
  opacity: 0;
  position: absolute;
  top: 50%;
  transform: scale(2);
  transition: all 250ms ease-out 0s;
  width: 50px;
  z-index: 20;
}
.video-list-item .video-title {
  bottom: 30px;
  font-size: 14px;
  left: 5px;
  overflow: hidden;
  position: absolute;
  right: 80px;
  text-overflow: ellipsis;
  transition: all 250ms ease-out 0s;
  white-space: nowrap;
  z-index: 5;
  text-align:left;
}
.video-list-item .video-info {
  bottom: 0;
  font-size: 12px;
  height: 55px;
  left: 0;
  overflow: hidden;
  position: absolute;
  right: 0;
}
.video-list-item .video-info .video-nickname {
  background: rgba(0, 0, 0, 0) url("http://i8.pdim.gs/215adf54fcfe0cc52e924a670373eca2.png") no-repeat scroll 0 3px;
  bottom: 7px;
  color: #787878;
  left: 5px;
  overflow: hidden;
  padding-left: 15px;
  position: absolute;
  right: 88px;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: left;
}
.video-list-item .video-info .video-number {
  background: rgba(0, 0, 0, 0) url("http://i9.pdim.gs/5e2630d66e51d54abb59f49198fa5dd1.png") no-repeat scroll 0 3px;
  bottom: 7px;
  color: #787878;
  padding-left: 17px;
  position: absolute;
  right: 5px;
}
.video-list-item .video-info .video-cate {
  bottom: 31px;
  color: #00a0ff;
  overflow: hidden;
  position: absolute;
  right: 5px;
  text-align: right;
  text-overflow: ellipsis;
  transition: all 250ms ease-out 0s;
  white-space: nowrap;
  width: 75px;
}

