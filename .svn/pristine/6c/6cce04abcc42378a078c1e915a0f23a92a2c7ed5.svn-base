 .unit {
  background: #f5f5f5 none repeat scroll 0 0;
  border: 1px solid #f5f5f5;
/* float: left;
margin: 0 25px 20px 0; */
  padding: 1px;
  text-align: left;

}

 .shadow {
  cursor: pointer;
  display: none;
  height: 195px;
  left: 0;
  opacity: 0;
  position: absolute;
  top: 0;
  width: 140px;
  z-index: 10;
}

 .unit .title {
  background: #fff none repeat scroll 0 0;
  color: #7e7e7e;
  font-size: 14px;
  overflow: hidden;
  padding: 5px 0 4px 6px;
  text-align: center;
  white-space: nowrap;
  word-break: break-all;
  margin: 0;
}

 .unit .thumb {
  display: block;
  position: relative;
  text-decoration: none;
}

 .unit a:hover  .title{
background-color: #f7f7f7;
color: #f70;

}
.thumb:hover{
    display: block;
    opacity: 0.8;
}
